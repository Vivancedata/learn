{
  "name": "eureka",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "prisma generate && next build",
    "build:ci": "DATABASE_URL=${DATABASE_URL:-postgresql://postgres:postgres@127.0.0.1:5432/learn} NEXT_TELEMETRY_DISABLED=1 NODE_ENV=production next build --webpack",
    "postinstall": "prisma generate",
    "build:webpack": "DATABASE_URL=${DATABASE_URL:-postgresql://postgres:postgres@127.0.0.1:5432/learn} NEXT_TELEMETRY_DISABLED=1 next build --webpack",
    "start": "next start",
    "lint": "eslint src --ext .ts,.tsx",
    "db:seed": "ts-node --project prisma/tsconfig.seed.json prisma/seed.ts",
    "db:migrate:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:unit:coverage": "npm test -- --runInBand --coverage --coverageDirectory=coverage/unit --coverageReporters=text-summary --coverageReporters=json-summary --testPathPatterns='src/lib/__tests__|src/types/__tests__' --coverageThreshold='{\"global\":{\"statements\":0,\"branches\":0,\"functions\":0,\"lines\":0}}' && node scripts/check-coverage-threshold.js coverage/unit/coverage-summary.json 85 unit",
    "test:integration:coverage": "npm test -- --runInBand --coverage --coverageDirectory=coverage/integration --coverageReporters=text-summary --coverageReporters=json-summary --testPathPatterns='src/app/api/__tests__|__tests__/api' --coverageThreshold='{\"global\":{\"statements\":0,\"branches\":0,\"functions\":0,\"lines\":0}}' && node scripts/check-coverage-threshold.js coverage/integration/coverage-summary.json 85 integration /src/app/api/",
    "smoke": "npm run lint && npm test -- --runInBand && npm run build:ci",
    "test:e2e": "bash scripts/test-e2e.sh",
    "test:e2e:coverage": "MIN_E2E_ASSERTION_COVERAGE=85 bash scripts/test-e2e.sh",
    "test:coverage:all": "npm run test:unit:coverage && npm run test:integration:coverage && npm run test:e2e:coverage",
    "ops:check": "bash scripts/ops-check.sh",
    "ops:check:prod": "BASE_URL=https://learn.vivancedata.com bash scripts/ops-check.sh",
    "ops:sentry:bootstrap": "bash scripts/bootstrap-sentry-read-checks.sh",
    "lint:content": "markdownlint-cli2 'content/**/*.md'",
    "build:mobile": "CAPACITOR_BUILD=true next build",
    "cap:sync": "npx cap sync",
    "cap:sync:ios": "npx cap sync ios",
    "cap:sync:android": "npx cap sync android",
    "cap:open:ios": "npx cap open ios",
    "cap:open:android": "npx cap open android",
    "cap:run:ios": "npx cap run ios",
    "cap:run:android": "npx cap run android",
    "mobile:ios": "npm run build:mobile && npm run cap:sync:ios && npm run cap:open:ios",
    "mobile:android": "npm run build:mobile && npm run cap:sync:android && npm run cap:open:android",
    "mobile:dev:ios": "npx cap run ios --livereload --external",
    "mobile:dev:android": "npx cap run android --livereload --external"
  },
  "dependencies": {
    "@capacitor/android": "^7.0.0",
    "@capacitor/app": "^7.0.0",
    "@capacitor/browser": "^7.0.0",
    "@capacitor/cli": "^7.0.0",
    "@capacitor/core": "^7.0.0",
    "@capacitor/haptics": "^7.0.0",
    "@capacitor/ios": "^7.0.0",
    "@capacitor/keyboard": "^7.0.0",
    "@capacitor/push-notifications": "^7.0.0",
    "@capacitor/splash-screen": "^7.0.0",
    "@capacitor/status-bar": "^7.0.0",
    "@prisma/adapter-libsql": "^7.4.1",
    "@prisma/adapter-pg": "^7.4.1",
    "@prisma/client": "^7.4.1",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-slot": "^1.1.2",
    "@sentry/nextjs": "^10.39.0",
    "@stripe/stripe-js": "^8.7.0",
    "@upstash/ratelimit": "^2.0.8",
    "@upstash/redis": "^1.36.2",
    "@vivancedata/ui": "github:Vivancedata/ui",
    "bcryptjs": "^3.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "gray-matter": "^4.0.3",
    "jose": "^6.1.3",
    "lucide-react": "^0.475.0",
    "next": "^16.1.3",
    "next-themes": "^0.4.4",
    "pg": "^8.18.0",
    "posthog-js": "^1.337.1",
    "posthog-node": "^5.24.9",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-markdown": "^10.0.0",
    "remark-gfm": "^4.0.1",
    "resend": "^6.9.1",
    "stripe": "^20.3.0",
    "swr": "^2.4.0",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animate": "^1.0.7",
    "web-push": "^3.6.7",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.9.1",
    "@types/bcryptjs": "^3.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.3.0",
    "@types/pg": "^8.16.0",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@types/web-push": "^3.6.4",
    "eslint": "^10.0.2",
    "eslint-config-next": "^16.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "markdownlint-cli2": "^0.21.0",
    "postcss": "^8",
    "prisma": "^7.4.1",
    "tailwindcss": "^4.2.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "ts-node": {
    "compilerOptions": {
      "module": "commonjs"
    }
  },
  "overrides": {
    "diff": "^8.0.3",
    "lodash": "^4.17.23",
    "hono": "^4.12.0",
    "@sentry/bundler-plugin-core": {
      "glob": "^11.1.0"
    },
    "@sentry/node": {
      "minimatch": "^10.2.2"
    }
  }
}
