{
  "name": "eureka",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "build:ci": "NEXT_DISABLE_TURBOPACK=1 NEXT_TELEMETRY_DISABLED=1 next build",
    "build:webpack": "NEXT_DISABLE_TURBOPACK=1 NEXT_TELEMETRY_DISABLED=1 next build",
    "start": "next start",
    "lint": "next lint",
    "db:seed": "ts-node --project prisma/tsconfig.seed.json prisma/seed-sqlite.ts",
    "prisma:studio": "prisma studio",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint:content": "markdownlint-cli2 'content/**/*.md'",
    "build:mobile": "CAPACITOR_BUILD=true next build",
    "cap:sync": "npx cap sync",
    "cap:sync:ios": "npx cap sync ios",
    "cap:sync:android": "npx cap sync android",
    "cap:open:ios": "npx cap open ios",
    "cap:open:android": "npx cap open android",
    "cap:run:ios": "npx cap run ios",
    "cap:run:android": "npx cap run android",
    "mobile:ios": "npm run build:mobile && npm run cap:sync:ios && npm run cap:open:ios",
    "mobile:android": "npm run build:mobile && npm run cap:sync:android && npm run cap:open:android",
    "mobile:dev:ios": "npx cap run ios --livereload --external",
    "mobile:dev:android": "npx cap run android --livereload --external"
  },
  "dependencies": {
    "@capacitor/android": "^7.0.0",
    "@capacitor/app": "^7.0.0",
    "@capacitor/browser": "^7.0.0",
    "@capacitor/cli": "^7.0.0",
    "@capacitor/core": "^7.0.0",
    "@capacitor/haptics": "^7.0.0",
    "@capacitor/ios": "^7.0.0",
    "@capacitor/keyboard": "^7.0.0",
    "@capacitor/push-notifications": "^7.0.0",
    "@capacitor/splash-screen": "^7.0.0",
    "@capacitor/status-bar": "^7.0.0",
    "@libsql/client": "^0.17.0",
    "@prisma/adapter-libsql": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-slot": "^1.1.2",
    "@sentry/nextjs": "^10.38.0",
    "@stripe/stripe-js": "^8.7.0",
    "@upstash/ratelimit": "^2.0.8",
    "@upstash/redis": "^1.36.2",
    "bcryptjs": "^3.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "gray-matter": "^4.0.3",
    "jose": "^6.1.3",
    "lucide-react": "^0.475.0",
    "next": "^16.1.3",
    "next-themes": "^0.4.4",
    "posthog-js": "^1.337.1",
    "posthog-node": "^5.24.9",
    "prisma": "^7.3.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-markdown": "^10.0.0",
    "remark-gfm": "^4.0.1",
    "resend": "^6.9.1",
    "stripe": "^20.3.0",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animate": "^1.0.7",
    "web-push": "^3.6.7",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.9.1",
    "@types/bcryptjs": "^3.0.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/jest": "^29.5.14",
    "@types/node": "^25.0.9",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "better-sqlite3": "^12.6.2",
    "eslint": "^9.39.2",
    "eslint-config-next": "15.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "markdownlint-cli2": "^0.17.0",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "ts-node": {
    "compilerOptions": {
      "module": "commonjs"
    }
  },
  "overrides": {
    "diff": "^8.0.3"
  }
}
